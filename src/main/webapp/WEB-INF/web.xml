<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<display-name>DeleteServlet</display-name>
  	<description></description>
  	<servlet-class>com.fssa.studentmanagementapp.servlet.DeleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddStudentServlet</servlet-name>
  	<display-name>AddStudentServlet</display-name>
  	<description></description>
  	<servlet-class>com.fssa.studentmanagementapp.servlet.AddStudentServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListingStudents</servlet-name>
  	<display-name>ListingStudents</display-name>
  	<description></description>
  	<servlet-class>com.fssa.studentmanagementapp.servlet.ListingStudents</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentList</servlet-name>
  	<display-name>StudentList</display-name>
  	<description></description>
  	<servlet-class>com.fssa.studentmanagementapp.servlet.StudentList</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddStudentServlet</servlet-name>
  	<url-pattern>/AddStudentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListingStudents</servlet-name>
  	<url-pattern>/ListingStudents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentList</servlet-name>
  	<url-pattern>/StudentList</url-pattern>
  </servlet-mapping>
</web-app>
